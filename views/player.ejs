<h1>players</h1>

<table class="table table-striped table-hover">
    <tr><th>FirstName</th><th>LastName</th><th>Number</th>

    <% for (var i = 0; i < players.length; i++) { %>
        <tr>
            <td><%= players[i].FirstName %></td>
            <td><%= players[i].LastName %></td>
            <td><%= players[i].Number %></td>
        </tr>
    <% } %>

</table>